void setup () {
  size (1000,600);
  background (124, 195, 255);
  fill (0);
  textSize (75);
  text ("Chan's Checkers!", width/5, height/3);
}

void draw () {
  strokeWeight (5);
  fill (255,0,0);
  rect (width/5, height-300, 200, 100);
  fill (0,0,255);
  rect (width/2, height-300, 300, 100);
  fill (0);
  textSize (50);
  text ("Start", width/5+40, height-230);
  text ("Instructions", width/2+10, height-230);
}
